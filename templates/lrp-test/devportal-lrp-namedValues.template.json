{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApimServiceName": {
      "type": "string"
    },
    "PolicyXMLBaseUrl": {
      "type": "string"
    },
    "NamedValues": {
      "type": "object"
    }
  },
  "resources": [
    {
      "properties": {
        "tags": [
          "key",
          "function",
          "auto"
        ],
        "secret": true,
        "displayName": "alzaslonfuncapp-key",
        "value": "[parameters('NamedValues').alzaslonfuncappkey]"
      },
      "name": "[concat(parameters('ApimServiceName'), '/alzaslonfuncapp-key')]",
      "type": "Microsoft.ApiManagement/service/properties",
      "apiVersion": "2019-01-01"
    },
    {
      "properties": {
        "tags": [
          "key",
          "function",
          "auto"
        ],
        "secret": true,
        "displayName": "lrp-test-key",
        "value": "[parameters('NamedValues').lrptestkey]"
      },
      "name": "[concat(parameters('ApimServiceName'), '/lrp-test-key')]",
      "type": "Microsoft.ApiManagement/service/properties",
      "apiVersion": "2019-01-01"
    },
    {
      "properties": {
        "secret": false,
        "displayName": "named",
        "value": "[parameters('NamedValues').named]"
      },
      "name": "[concat(parameters('ApimServiceName'), '/named')]",
      "type": "Microsoft.ApiManagement/service/properties",
      "apiVersion": "2019-01-01"
    },
    {
      "properties": {
        "tags": [
          "key",
          "function",
          "auto"
        ],
        "secret": true,
        "displayName": "vifedofunctionapptest-key",
        "value": "[parameters('NamedValues').vifedofunctionapptestkey]"
      },
      "name": "[concat(parameters('ApimServiceName'), '/vifedofunctionapptest-key')]",
      "type": "Microsoft.ApiManagement/service/properties",
      "apiVersion": "2019-01-01"
    },
    {
      "properties": {
        "tags": [
          "key",
          "function",
          "auto"
        ],
        "secret": true,
        "displayName": "vifedofunctionapp-v3-1-key",
        "value": "[parameters('NamedValues').vifedofunctionappv31key]"
      },
      "name": "[concat(parameters('ApimServiceName'), '/vifedofunctionapp-v3-1-key')]",
      "type": "Microsoft.ApiManagement/service/properties",
      "apiVersion": "2019-01-01"
    }
  ]
}